{
  "name": "AIflow-detection",
  "type": "AIflow:activity",
  "version": "1.0.0",
  "title": "Object detection",
  "description": "Object detection",
  "display": {
    "icon": "object-detection.svg"
  },
  "settings": [
    {
      "name": "name",
      "type": "string",
      "required": true,
      "description": "The name of the shared attribute"
    }
  ],
  "input": [
    {
      "name": "image",
      "type": "image",
      "description": "The image to run detection on"
    },
    {
      "name": "rois",
      "type": "array",
      "description": "Region of interests"
    },
    {
      "name": "cameraId",
      "type": "string",
      "description": "The camera identifier"
    },
    {
      "name": "groupId",
      "type": "string",
      "description": "The camera group identifier"
    }
  ],
  "output": [
    {
      "name": "results",
      "type": "object",
      "description": "The detected results"
    }
  ]
}
