{
  "name": "MyProxy",
  "type": "AIflow:app",
  "version": "1.0.0",
  "description": "This is a simple proxy.",
  "properties": null,
  "channels": null,
  "triggers": [
    {
      "name": "AIflow-rest",
      "id": "MyProxy",
      "ref": "github.com/r2d2-ai/trigger/net/rest",
      "settings": {
        "port": "9096"
      },
      "handlers": [
        {
          "settings": {
            "method": "GET",
            "path": "/endpoint"
          },
          "actions": [
            {
              "ref": "github.com/r2d2-ai/aiflow/action/microgateway/",
              "settings": {
                "uri": "pattern://CustomPattern"
              }
            }
          ]
        }
      ]
    }
  ],
  "resources": [],
  "actions": []
}
